    PUSHA aug(300000)
    PUSHA aug(4000000)

    // {#}D, {#}S uses absolute immediates:
    WRPIN 0, 0
    WRPIN 100, 100
    WRPIN 200, 200
    WRPIN aug(0), aug(0)
    WRPIN aug(100), aug(100)
    WRPIN aug(200), aug(200)
    WRPIN aug(1000), aug(1000)
    WRPIN aug(20000), aug(20000)
    WRPIN aug(300000), aug(300000)
    WRPIN aug(4000000), aug(4000000)

    // {#}D, {#}S uses pc-rel addressing, cogexec:
    CALLPA 0, 0
    CALLPA 100, 100
    CALLPA 200, 200

    CALLPA 0, aug(0x0)
    CALLPA 0, aug(0x100)
    CALLPA 0, aug(0x200)
    CALLPA aug(0x0), aug(0x0)
    CALLPA aug(0x100), aug(0x100)
    CALLPA aug(0x200), aug(0x200)

