

normal:
    JMP destination // must select JMP D {WC/WZ/WCZ}
    JMP value       // must select JMP #{\}A

inverted:
    JMP *destination // must select JMP #{\}A
    JMP &value       // must select JMP D {WC/WZ/WCZ}

useless:
    JMP &destination // must select JMP #{\}A
    JMP *value       // must select JMP D {WC/WZ/WCZ}

destination:
    NOP

var value:
    long 0